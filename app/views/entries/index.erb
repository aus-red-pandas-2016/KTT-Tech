<ul class="entries-list">
  <% @entries.each do |entry| %>
    <li class="entry-item">
      <span class="entry-title"><%= entry.title %></span>
      <span class="entry-date"><%= "Written on #{entry.date_written}." %></span>
      <p class="entry-stub"><%= entry.stubbed_body %></p>
    </li>
  <% end %>
</ul>
