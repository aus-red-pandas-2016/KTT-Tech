<h1><%= @entry.title %></h1>
<span><%= "Written on #{long_form_date(@entry.created_at)}." %></span>
<p><%= @entry.body %></p>
<a href="/entries/<%= @entry.id %>/edit">edit</a>
<form method="post" action="/entries/<%= @entry.id %>">
  <input type="hidden" name="_method" value="delete">
  <input type="submit" value="delete">
</form>
